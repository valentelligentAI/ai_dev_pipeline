{
  "name": "ai-dev-pipeline",
  "version": "1.0.0",
  "description": "AI Development Pipeline with Setup Assistance",
  "main": "dist/index.js",
  "bin": {
    "setup-assistant": "./dist/cli/setup-assistant.js",
    "test-generator": "./dist/cli/test-generator-cli.js",
    "run-diagnostics": "./dist/cli/run-diagnostics.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:coverage": "jest --coverage",
    "test:unit": "cross-env NODE_ENV=test jest --testMatch='**/*.test.ts'",
    "test:integration": "cross-env NODE_ENV=test jest --testMatch='**/*.integration.test.ts'",
    "test:db": "cross-env NODE_ENV=test jest --testMatch='**/database/__tests__/*.test.ts'",
    "test:db:unit": "cross-env NODE_ENV=test jest --testMatch='**/database/__tests__/client.test.ts'",
    "test:db:int": "cross-env NODE_ENV=test jest --testMatch='**/context/__tests__/postgres-context-sync.test.ts'",
    "test:context-sync": "cross-env NODE_ENV=test jest --testMatch='**/context-sync*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "analyze-bundle": "source-map-explorer dist/**/*.js",
    "setup": "node dist/cli/setup-assistant.js check",
    "generate:tests": "node dist/cli/test-generator-cli.js generate",
    "prepare": "husky install",
    "validate:types": "ts-node src/utils/type-validator.ts",
    "analyze:types": "ts-node src/cli/run-diagnostics.ts",
    "fix:types": "ts-node src/cli/run-diagnostics.ts --fix",
    "db:test-connection": "ts-node scripts/test-db-connection.ts",
    "db:setup-postgres": "ts-node scripts/setup-postgres.ts",
    "db:troubleshoot": "ts-node scripts/troubleshoot-postgres.ts",
    "db:init": "npm run db:setup-postgres && ts-node scripts/init-database.ts",
    "db:migrate": "ts-node scripts/migrate-database.ts",
    "db:migrate:rollback": "ts-node scripts/migrate-database.ts --rollback",
    "db:migrate:status": "ts-node scripts/migrate-database.ts --status",
    "db:reset": "ts-node scripts/init-database.ts --reset && npm run db:migrate",
    "db:test:setup": "cross-env NODE_ENV=test ts-node scripts/setup-test-db.ts",
    "db:test:init": "cross-env NODE_ENV=test ts-node scripts/init-database.ts",
    "pretest": "npm run db:test:setup",
    "pretest:unit": "npm run db:test:setup",
    "pretest:integration": "npm run db:test:setup",
    "pretest:db": "npm run db:test:setup",
    "pretest:db:int": "npm run db:test:setup",
    "pretest:context-sync": "npm run db:test:setup"
  },
  "dependencies": {
    "@types/redis": "^4.0.10",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "glob": "^8.0.3",
    "ora": "^5.4.1",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "@babel/types": "^7.26.3",
    "@jest/globals": "^29.7.0",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.68",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@typescript-eslint/types": "^8.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^28.9.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "pg": "^8.13.1",
    "prettier": "^2.8.1",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
